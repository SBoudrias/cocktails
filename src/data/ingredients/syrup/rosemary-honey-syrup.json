{
  "$schema": "../../../schemas/ingredient.schema.json",
  "name": "Rosemary honey syrup",
  "type": "syrup",
  "ingredients": [
    {
      "name": "honey",
      "type": "other",
      "quantity": {
        "amount": 2,
        "unit": "cup"
      }
    },
    {
      "name": "Water",
      "type": "other",
      "quantity": {
        "amount": 1,
        "unit": "cup"
      }
    },
    {
      "name": "Rosemary Sprig",
      "type": "spice",
      "quantity": {
        "amount": 5,
        "unit": "unit"
      }
    }
  ],
  "instructions": [
    "Combine all ingredients in a small saucepan.",
    "Heat on low and allow to steep but not reduce.",
    "Refrigerate."
  ]
}
