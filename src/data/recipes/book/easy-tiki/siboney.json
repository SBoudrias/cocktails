{
  "$schema": "../../../../schemas/recipe.schema.json",
  "name": "Siboney",
  "ingredients": [
    {
      "name": "Two James Doctor Bird rum",
      "type": "spirit",
      "quantity": { "amount": 1.5, "unit": "oz" }
    },
    {
      "name": "Planteray Stiggins' Fancy pineapple rum",
      "type": "spirit",
      "quantity": { "amount": 0.5, "unit": "oz" }
    },
    {
      "name": "pineapple juice",
      "type": "juice",
      "quantity": { "amount": 1, "unit": "oz" }
    },
    {
      "name": "lime juice",
      "type": "juice",
      "quantity": { "amount": 0.75, "unit": "oz" }
    },
    {
      "name": "passion fruit syrup",
      "type": "syrup",
      "quantity": { "amount": 0.5, "unit": "oz" }
    },
    {
      "name": "Demerara Syrup",
      "brix": 66,
      "type": "syrup",
      "quantity": { "amount": 0.25, "unit": "oz" }
    }
  ],
  "preparation": "shaken",
  "served_on": "up",
  "glassware": "coupe",
  "instructions": [
    "Combine all the ingredients in a cocktail shaker.",
    "Add ice and shake until chilled, about 10 seconds.",
    "Strain into a coupe or cocktail glass."
  ],
  "attributions": [
    { "source": "Antonio Martinez", "relation": "recipe author" },
    { "source": "Bootlegger Tiki, Palm Springs", "relation": "bar" }
  ],
  "refs": [{ "type": "book", "title": "Easy Tiki", "page": 84 }]
}
